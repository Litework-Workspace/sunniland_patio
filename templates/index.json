          }
        },
        "template--17486863663400__654c65c4-1510-4fc8-8c6c-4733e02e3b62-1673056524ee598275-1": {
          "type": "testimonial",
          "settings": {
            "quote": "<p>Great business to deal with - great price and no hassles.  Love it - thank you!<\/p>",
            "name": "Kathy Robison"
          }
        },
        "template--17486863663400__654c65c4-1510-4fc8-8c6c-4733e02e3b62-1673056524ee598275-2": {
